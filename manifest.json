{
   "background": {
      "scripts": [ "data.js", "cperapera.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/toolbar-disabled.png",
      "default_title": "Cantonese Popup Dictionary"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "cperacontent.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Cantonese-English pop-up dictionary for Chrome",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "name": "Cantonese Popup Dictionary",
   "permissions": [ "tabs", "http://*/*" ],
   "version": "0.0.3",
   "web_accessible_resources": [ "css/*", "cperacontent.js", "images/*" ]
}
